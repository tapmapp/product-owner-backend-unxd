generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["selectRelationCount"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id String @id @default(uuid())
  productBrand String
  productReference String
  productId String @unique
}

model Account {
  id String @id @default(uuid())
  accountEmail String @unique
  accountPassword String
}
